{
  "expo": {
    "name": "Islamku",
    "slug": "islamku",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/islamku_2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/islamku_2.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A148C"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/islamku_logos.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.islamku.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/islamku_logos.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
